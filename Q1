import random

odd_list = random.sample(range(1, 100, 2), 5)
even_list = random.sample(range(2, 100, 2), 4)
odd_list[2] = even_list.copy()

flattened = []
for item in odd_list:
    if isinstance(item, list):
        flattened.extend(item)
    else:
        flattened.append(item)
flattened.sort()
print(flattened)
