employees = {
    101: ("HR", 30000),
    102: ("IT", 50000),
    103: ("HR", 25000),
    104: ("IT", 55000),
    105: ("Finance", 40000)
}

departments = {}

for roll, (dept, salary) in employees.items():
    if dept not in departments:
        departments[dept] = []
    departments[dept].append(salary)

for dept in departments:
    print(f"{dept} -> Min Salary: {min(departments[dept])}, Max Salary: {max(departments[dept])}")
