import csv

# Read CSV data into a dictionary
students_dict = {}
with open('students.csv', mode='r') as file:
    reader = csv.DictReader(file)
    for row in reader:
        rollno = row['RollNo']
        students_dict[rollno] = {'Name': row['Name'], 'Subject1': row['Subject1'], 'Subject2': row['Subject2'], 'Subject3': row['Subject3'], 'Total': int(row['Subject1']) + int(row['Subject2']) + int(row['Subject3'])}

print(students_dict)
